{
  "name": "midwayladiswebserver",
  "version": "1.1.0",
  "description": "ladis网站后台服务",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts  --entryFile=bootstrap.js",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "check": "luckyeye",
    "build:docker": "npm run build && sudo docker build -t midwayladiswebserver .",
    "run:docker": "sudo docker stop midwayladiswebserver && sudo docker rm midwayladiswebserver && sudo docker run -itd --name midwayladiswebserver -p 9007:9007 --volume /home/cc/Web/midwayladiswebserver/static:/app/static midwayladiswebserver"
  },
  "dependencies": {
    "@cairui/wx-sdk": "^1.0.6",
    "@midwayjs/bootstrap": "^2.13.4",
    "@midwayjs/cache": "^2.13.4",
    "@midwayjs/core": "^2.13.4",
    "@midwayjs/decorator": "^2.13.2",
    "@midwayjs/koa": "^2.13.4",
    "@midwayjs/typegoose": "^2.13.5",
    "@typegoose/typegoose": "^9.11.0",
    "axios": "^0.21.1",
    "cache-manager": "^3.4.3",
    "cheerio": "^1.0.0-rc.10",
    "crypto-js": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa-body": "^4.2.0",
    "mongoose": "^6.5.1",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "types-uart": "^1.1.7"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.2.71",
    "@midwayjs/luckyeye": "^1.0.0",
    "@midwayjs/mock": "^2.3.0",
    "@types/cache-manager": "^3.4.0",
    "@types/cheerio": "^0.22.29",
    "@types/crypto-js": "^4.0.1",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "14",
    "@types/node-fetch": "^2.5.10",
    "@types/rimraf": "^3.0.0",
    "cross-env": "^6.0.0",
    "jest": "^26.4.0",
    "mwts": "^1.0.5",
    "ts-jest": "^26.2.0",
    "types-uart": "^1.0.22",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "wgcairui@icloud.com",
  "license": "MIT"
}
