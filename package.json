{
  "name": "midwayladiswebserver",
  "version": "1.0.0",
  "description": "ladis网站后台服务",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "check": "luckyeye",
    "build:docker": "npm run build && sudo docker build -t midwayladiswebserver .",
    "run:docker": "sudo docker stop midwayladiswebserver && sudo docker rm midwayladiswebserver && sudo docker run -itd --name midwayladiswebserver -p 9007:9007 --volume /home/cc/Web/midwayladiswebserver/static:/app/static midwayladiswebserver"
  },
  "dependencies": {
    "@cairui/wx-sdk": "^1.0.3",
    "@midwayjs/bootstrap": "^2.3.0",
    "@midwayjs/cache": "^2.10.18",
    "@midwayjs/core": "^2.3.0",
    "@midwayjs/decorator": "^2.3.0",
    "@midwayjs/koa": "^2.3.0",
    "@midwayjs/typegoose": "^2.10.14",
    "@typegoose/typegoose": "^7.6.0",
    "cache-manager": "^3.4.3",
    "crypto-js": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa-bodyparser": "^4.3.0",
    "mongoose": "5.10.18",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.2.70",
    "@midwayjs/luckyeye": "^1.0.0",
    "@midwayjs/mock": "^2.3.0",
    "@types/cache-manager": "^3.4.0",
    "@types/crypto-js": "^4.0.1",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/mongoose": "5.10.5",
    "@types/node": "14",
    "@types/node-fetch": "^2.5.10",
    "cross-env": "^6.0.0",
    "jest": "^26.4.0",
    "mwts": "^1.0.5",
    "ts-jest": "^26.2.0",
    "types-uart": "^1.0.20",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "wgcairui@icloud.com",
  "license": "MIT"
}
